<section class='frame frame_md frame__registration'>
  <h2>Регистрация</h2>
  <form on-submit='send'>
      <div  class='frame__registration_cols'>
    {{input_mail}}
    {{input_login}}
    {{input_name}}
    {{input_lastname}}
    {{input_password}}
    {{input_phone}}
